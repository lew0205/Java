package chapter06;

public class Info1 {

	public static void main(String[] args) {
		PeopleData d1 = new PeopleData();
		d1.name="홍길동";
		d1.age=22;
		d1.gender='남';
		System.out.printf("이름 : %s, 나이 : %d, 성별 : %c\n",d1.name,d1.age,d1.gender);
	}

}
class PeopleData{
	String name;
	int age;
	char gender;
}