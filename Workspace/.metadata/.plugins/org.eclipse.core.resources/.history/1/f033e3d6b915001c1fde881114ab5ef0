package chapter08;

class Printer {
	int numOfPapers = 0;

	public void print(int amount) {
		numOfPapers-=amount;
	}
}

public class PrinterTest {

	public static void main(String[] args) {
		Printer pr = new Printer();
		pr.numOfPapers = 100;
		pr.print(70);
		System.out.println(pr.numOfPapers);
	}

}
