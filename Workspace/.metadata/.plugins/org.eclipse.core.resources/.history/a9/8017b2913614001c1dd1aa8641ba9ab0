package chapter07;

public class Timetest {

	public static void main(String[] args) {
		Time t1 = new Time();
		t1.hour = 100;
		System.out.println(t1.hour);
	}

}
class Time{
	private int hour;
	int minute;
	float second;
	
	void setHour(int hour) {
		if(hour<0||hour>23) return;
		this.hour=hour;
	}
	int getHour() {
		return hour;
	}
}
